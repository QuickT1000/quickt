{
  "include": [
    "src/**/*",
    "jest.setup.ts"
  ],
  "outDir": "./build",
  "rootDir": "./client",
  "exclude":[
    "./node_modules",
    "react/require-default-props"
  ],
  "compilerOptions": {
    "jsx": "react",
    "skipLibCheck": true,
    "paths": {
      "@components/*": ["./src/components/*"],
      "@services/*": ["./src/services/*"],
    },
  }
}